];





const v_timeout = 6000;
const v_fade_timeout = 3500;






//
// program
//

var v_rot_index;
var v_interval_id;
var v_fadeinterval_id;
var v_len;
var v_target;
var v_collapse;
var v_cnt;


//Main
window.addEventListener("DOMContentLoaded", function(){
    if (typeof v_banners !== 'undefined' && v_banners.length > 0) {
	v_banners_full = v_banners_full.concat(v_banners);
    }
    const v_pri_banners_no = v_banners_full.length;

    if (typeof v_banners_sec !== 'undefined' && v_banners_sec.length > 0) {
	v_banners_full = v_banners_full.concat(v_banners_sec);
    }

    v_banners_full = v_banners_full.concat(v_banners_dfl_sec);

    v_rot_index = Math.floor(Math.random() * v_pri_banners_no);

    v_len=v_banners_full.length
    v_target = document.getElementById("v_banner_text");
    v_collapse = document.getElementById("v_banner_pos");
    v_cnt = document.getElementById("v_cnt");


    v_launch_cycle();


});






function v_show()
{
// show current banner fully
    v_target.style.opacity = 1;
    v_target.innerHTML = v_banners_full[v_rot_index] ;
    v_cnt.innerHTML = (v_rot_index + 1) + '/' + v_len;
};



function v_next()
{
// next banner
    v_rot_index = (v_rot_index + 1) % v_len;
    v_show();
};

function v_prev()
{
// previuos banner
    v_rot_index = (v_rot_index == 0) ? (v_len - 1) : (v_rot_index-1)
    v_show();
};


function v_launch_cycle()
{
    v_stop_cycle();
    v_show();
    v_interval_id = setInterval(v_dim_switch_start,v_timeout);
};


function v_dim_switch_start()
{
    v_stop_cycle();
    v_target.style.opacity = 0;
    v_fadeinterval_id = setInterval(v_banner_switch,v_fade_timeout);
};


function v_banner_switch()
{
    v_stop_cycle();
    v_next();
    v_launch_cycle();
};



function v_stop_cycle()
{
    clearInterval (v_interval_id);
    clearInterval (v_fadeinterval_id);
    v_target.style.opacity = 1;

};

function v_close()
{
     // close display
    v_stop_cycle();
    v_collapse.style.display = 'none';
    v_collapse.innerHTML = '';
};


</script>





